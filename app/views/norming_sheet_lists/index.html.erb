<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      Mina(or someone)'s Norming Sheets
    </h1>
  </div>
</div>

<hr>

<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h2>
      Your norming sheets
    </h2>

 <hr>
<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Student
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @norming_sheet_lists.each do |norming_sheet_list| %>
      <tr>
        <td>
          <%= norming_sheet_list.id %>
        </td>

        <td>
          <%= norming_sheet_list.student_id %>
        </td>

        <td>
          <%= time_ago_in_words(norming_sheet_list.created_at) %> ago
        </td>
        <td>
          <%= time_ago_in_words(norming_sheet_list.updated_at) %> ago
        </td>

        <td>
          <a href="/norming_sheet_lists/<%= norming_sheet_list.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

<hr>
